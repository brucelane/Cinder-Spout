<?xml version="1.0" encoding="UTF-8" ?>
<cinder>
<block
	name="Spout"
	id="fr.batchass.spout"
	author="Bruce Lane/Eric Renaud-Houde"
	license="MIT"
	summary="Spout block for Cinder"
	version="1.0.20200530"
	url="https://github.com/brucelane/Cinder-Spout"
	git="git://github.com/brucelane/Cinder-Spout.git"
	>
	<supports os="msw" />
	<includePath>include</includePath>
	<headerPattern>include/*.h</headerPattern>
